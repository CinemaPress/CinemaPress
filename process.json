{
  "apps": [
    {
      "name": "example.com",
      "script": "./app.js",
      "instances": "1",
      "exec_mode": "cluster",
      "cwd": "./",
      "error_file": "./log/err.log",
      "out_file": "./log/out.log",
      "time": true,
      "watch": [
        "app.js",
        "themes/*/views/*.ejs",
        "themes/*/views/*/*.ejs",
        "themes/*/views/*/*/*.ejs",
        "lib",
        "modules",
        "routes"
      ],
      "watch_delay": 1000,
      "watch_options": {
        "followSymlinks": false
      },
      "vizion": false,
      "env": {
        "NODE_ENV": "production",
        "CP_ALL": "_example_com_",
        "CP_XMLPIPE2": "xmlpipe2_example_com",
        "CP_RT": "rt_example_com",
        "CP_CONTENT": "content_example_com",
        "CP_COMMENT": "comment_example_com",
        "CP_USER": "user_example_com",
        "CP_VER": "1",
        "NO_CACHE": ""
      }
    }
  ]
}
