                                        </div>

                                        <script>
                                            document.addEventListener('DOMContentLoaded', function() {
                                                var cgs = document.querySelector('[name="config.default.categories.genres"]');
                                                var cg = new Tagify(cgs, {whitelist : []});
                                                cg.DOM.input.classList.add('form-control');

                                                var cys = document.querySelector('[name="config.default.categories.years"]');
                                                var cy = new Tagify(cys, {whitelist : []});
                                                cy.DOM.input.classList.add('form-control');

                                                var ccs = document.querySelector('[name="config.default.categories.countries"]');
                                                var cc = new Tagify(ccs, {whitelist : []});
                                                cc.DOM.input.classList.add('form-control');

                                                var editInput = document.querySelectorAll('.edit');
                                                editInput.forEach(function(ei) {
                                                    ei.addEventListener('click', function() {
                                                        var inp = document.querySelectorAll('input');
                                                        var tag = document.querySelectorAll('.tagify__input');
                                                        var tags = document.querySelectorAll('.tagify--noAnim');
                                                        inp.forEach(function(i) {
                                                            if (i.placeholder === '...') {
                                                                if (i.style.display === 'block') {
                                                                    i.style.display = 'none';
                                                                    i.setAttribute('class', '');
                                                                    tag.forEach(function(t) {t.style.display = 'block';});
                                                                    tags.forEach(function(t) {t.style.display = 'block';});
                                                                }
                                                                else {
                                                                    i.style.display = 'block';
                                                                    i.setAttribute('class', 'form-control');
                                                                    tag.forEach(function(t) {t.style.display = 'none';});
                                                                    tags.forEach(function(t) {t.style.display = 'none';});
                                                                }
                                                            }
                                                        });
                                                    });
                                                });
                                            });
                                        </script>

                                    </div>
                                </div>
                            </div>
