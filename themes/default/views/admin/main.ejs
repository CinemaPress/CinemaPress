<body>
<div id="container" class="clearfix">

    <%- include('includes/left.ejs'); -%>

    <%- include('includes/right.ejs'); -%>

    <div id="main" class="clearfix">

        <%- include('includes/header-main.ejs'); -%>

        <div id="content" class="clearfix">

            <%- include('includes/header-sec.ejs'); -%>

            <div class="window">
                <div class="actionbar">
                    <div class="pull-left">
                        <a href="javascript:void(0)" class="btn" data-toggle-sidebar="left">
                            <i class="fa fa-chevron-left"></i>
                        </a>
                        <a href="/<%- config.urls.admin %>/main" class="btn">
                            <%= __('Основные настройки веб-сайта') %>
                        </a>
                        <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html" class="btn"
                           title="<%= __('Информация о параметрах настройки веб-сайта') %>" target="_blank">
                            <i class="fa fa-info-circle text-muted"></i>
                        </a>
                    </div>
                    <div class="pull-right">
                        <a href="javascript:void(0)" class="btn" data-toggle-sidebar="right">
                            <i class="fa fa-chevron-right"></i>
                        </a>
                    </div>
                </div>

                <div class="inner-padding">

                    <div class="row">
                        <div class="col-sm-12">
                            <div class="cinema-box">
                                <div class="cinema-box-header">
                                    <div class="cinema-label">
                                        <i class="fab fa-expeditedssl"></i>&nbsp;&nbsp;&nbsp;<%= __('Протокол') %>
                                    </div>
                                    <div class="cinema-label hidden-xs" style="float: right">
                                        <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html"
                                           class="csstip" data-title="<%= __('Основной протокол веб-сайта') %>"
                                           target="_blank">
                                            <i class="fa fa-info-circle"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="inner-padding">
                                    <div class="row">
                                        <div class="col-lg-12 col-xs-12 text-center">
                                            <div class="inline-labels">
                                                <label>
                                                    <input name="config.protocol" value="http://"
                                                    <% if (config.protocol === 'http://') { %> checked=""
                                                            <% } %>
                                                           type="radio"><span></span> HTTP
                                                </label>
                                                <label>
                                                    <input name="config.protocol" value="https://"
                                                    <% if (config.protocol === 'https://') { %> checked=""
                                                            <% } %>
                                                           type="radio"><span></span> HTTPS
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="spacer-10"></div>

                        <div class="col-sm-12">
                            <div class="cinema-box">
                                <div class="cinema-box-header">
                                    <div class="cinema-label">
                                        <i class="fa fa-link"></i>&nbsp;&nbsp;&nbsp;<%= __('Домен') %>
                                    </div>
                                    <div class="cinema-label hidden-xs" style="float: right">
                                        <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html"
                                           class="csstip"
                                           data-title="<%= __('Настройка работы веб-сайта на отдельных доменах и поддоменах для поисковых систем и пользователей') %>"
                                           target="_blank">
                                            <i class="fa fa-info-circle"></i>
                                        </a>
                                    </div>
                                </div>
                                <div class="inner-padding">
                                    <div class="row">
                                        <div class="col-xs-12">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="fas fa-users text-muted" style="color:#4B5B6C !important"></i>&nbsp;&nbsp;<%= __('Домен для людей') %>:
                                                </span>
                                                <input name="config.subdomain"
                                                       value="<%= config.subdomain.replace(/\.$/, '') %>"
                                                       class="form-control input_url text-right" type="text"
                                                       placeholder="hd" style="border-right: 0">
                                                <span class="input-group-addon" style="background-color: #1a2035; border-right: 0;border-left: 0;">
                                                    .
                                                </span>
                                                <input value="<%= config.domain %>"
                                                       class="form-control input_url text-left" type="text"
                                                       placeholder="<%= config.domain %>" style="background-color: #1a2035; border-left: 0" disabled="disabled">
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-sm-12">
                                            <div class="faq">
                                                <article class="faq-group">
                                                    <header class="text-muted text-left">
                                                        <i class="fa fa-plus"></i>
                                                        <h3><%= __('Случайный поддомен каждый день') %></h3>
                                                    </header>
                                                    <div class="faq-content clearfix">
                                                        <div class="row">
                                                            <div class="col-xs-4 text-center">
                                                                <label>
                                                                    <input name="config.random" value="0" <% if (!config.random) { %> checked="" <% } %> type="radio"><span></span> <%= __('Нет') %>
                                                                </label>
                                                            </div>
                                                            <div class="col-xs-4 text-center">
                                                                <label>
                                                                    <input name="config.random" value="3" <% if (config.random === 3) { %> checked="" <% } %> type="radio"><span></span> <%= __('Да') %> 3
                                                                </label>
                                                            </div>
                                                            <div class="col-xs-4 text-center">
                                                                <label>
                                                                    <input name="config.random" value="4" <% if (config.random === 4 || config.random === 1) { %> checked="" <% } %> type="radio"><span></span> <%= __('Да') %> 4
                                                                </label>
                                                            </div>

                                                            <div class="col-xs-12">
                                                                <div class="faq" id="subdomains">

                                                                    <div class="spacer-10"></div>

                                                                    <article class="faq-group">
                                                                        <header class="text-muted text-left" id="header_subdomains">
                                                                            <i class="fa fa-plus"></i>
                                                                            <h3 id="current_subdomain"><%= __('Случайный поддомен каждый день') %></h3>
                                                                        </header>
                                                                        <div class="faq-content clearfix" id="div_subdomains">
                                                                            <div class="row">
                                                                                <div class="col-sm-4">
                                                                                    <div class="helper-text-box text-left list_subdomains1">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-4">
                                                                                    <div class="helper-text-box text-left list_subdomains2">
                                                                                    </div>
                                                                                </div>
                                                                                <div class="col-sm-4">
                                                                                    <div class="helper-text-box text-left list_subdomains3">
                                                                                    </div>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </article>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </article>
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-xs-12">
                                            <div class="input-group">
                                                <span class="input-group-addon">
                                                    <i class="fas fa-eye text-muted" style="color:#4B5B6C !important"></i>&nbsp;&nbsp;<%= __('Домен для ботов') %>:
                                                </span>
                                                <input name="config.botdomain"
                                                       value="<%= config.botdomain.replace(/\.$/, '') %>"
                                                       class="form-control input_url text-right" type="text"
                                                       placeholder="<%= config.language %>" style="border-right: 0">
                                                <span class="input-group-addon" style="background-color: #1a2035; border-right: 0;border-left: 0;">
                                                    .
                                                </span>
                                                <input name="config.bomain"
                                                       value="<%= config.bomain %>"
                                                       class="form-control input_url text-left" type="text"
                                                       placeholder="<%= config.bomain ? config.bomain : config.domain %>" style="border-left: 0">
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-xs-12">
                                            <div class="input-group csstip2" data-title="<%= __('Альтернативный домен, который видит только поисковый бот') %>">
                                                <span class="input-group-addon">
                                                    <i class="fas fa-eye-slash text-muted" style="color:#4B5B6C !important"></i>&nbsp;&nbsp;<%= __('Домен для ботов') %>:
                                                </span>
                                                <input name="config.alt.botdomain"
                                                       value="<%= config.alt.botdomain.replace(/\.$/, '') %>"
                                                       class="form-control input_url text-right" type="text"
                                                       placeholder="my" style="border-right: 0">
                                                <span class="input-group-addon" style="background-color: #1a2035; border-right: 0;border-left: 0;">
                                                    .
                                                </span>
                                                <input name="config.alt.bomain"
                                                       value="<%= config.alt.bomain %>"
                                                       class="form-control input_url text-left" type="text"
                                                       placeholder="<%= config.alt.bomain ? config.alt.bomain : (config.bomain || config.domain) %>" style="border-left: 0">
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-sm-12">
                                            <div class="faq">
                                                <article class="faq-group">
                                                    <header class="text-muted text-left">
                                                        <i class="fa fa-plus"></i>
                                                        <h3><%= __('Настройка доменов только для России') %></h3>
                                                    </header>
                                                    <div class="faq-content clearfix">
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <div class="cinema-box">
                                                                    <div class="cinema-box-header">
                                                                        <div class="cinema-label"><i class="fas fa-search-location"></i>&nbsp;&nbsp;&nbsp;<%= __('Настройка доменов только для России') %>
                                                                        </div>
                                                                        <div class="cinema-label hidden-xs" style="float: right">
                                                                            <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html"
                                                                               class="csstip"
                                                                               data-title="<%= __('Настройка отдельных доменов для пользователей с российским IP и поисковых ботов') %>"
                                                                               target="_blank">
                                                                                <i class="fa fa-info-circle"></i>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                    <div class="inner-padding">
                                                                        <div class="row">
                                                                            <div class="col-xs-12">
                                                                                <div class="input-group">
                                                                                    <span class="input-group-addon">
                                                                                        <i class="fas fa-users text-muted" style="color:#4B5B6C !important"></i>&nbsp;&nbsp;<%= __('Домен для людей') %>:
                                                                                    </span>
                                                                                    <input name="config.ru.subdomain"
                                                                                           value="<%= config.ru.subdomain.replace(/\.$/, '') %>"
                                                                                           class="form-control input_url text-right" type="text"
                                                                                           placeholder="hd" style="border-right: 0">
                                                                                    <span class="input-group-addon" style="background-color: #1a2035; border-right: 0;border-left: 0;">
                                                                                        .
                                                                                    </span>
                                                                                    <input name="config.ru.domain"
                                                                                           value="<%= config.ru.domain %>"
                                                                                           class="form-control input_url text-left" type="text"
                                                                                           placeholder="<%= config.domain %>" style="border-left: 0">
                                                                                </div>
                                                                            </div>

                                                                            <div class="spacer-10"></div>

                                                                            <div class="col-xs-4 text-center">
                                                                                <label>
                                                                                    <input name="config.ru.random" value="0" <% if (!config.ru.random) { %> checked="" <% } %> type="radio"><span></span> <%= __('Нет') %>
                                                                                </label>
                                                                            </div>
                                                                            <div class="col-xs-4 text-center">
                                                                                <label>
                                                                                    <input name="config.ru.random" value="3" <% if (config.ru.random === 3) { %> checked="" <% } %> type="radio"><span></span> <%= __('Да') %> 3
                                                                                </label>
                                                                            </div>
                                                                            <div class="col-xs-4 text-center">
                                                                                <label>
                                                                                    <input name="config.ru.random" value="4" <% if (config.ru.random === 4 || config.ru.random === 1) { %> checked="" <% } %> type="radio"><span></span> <%= __('Да') %> 4
                                                                                </label>
                                                                            </div>

                                                                            <div class="spacer-10"></div>

                                                                            <div class="col-xs-12">
                                                                                <div class="input-group">
                                                                                    <span class="input-group-addon">
                                                                                        <i class="fas fa-eye text-muted" style="color:#4B5B6C !important"></i>&nbsp;&nbsp;<%= __('Домен для ботов') %>:
                                                                                    </span>
                                                                                    <input name="config.ru.botdomain"
                                                                                           value="<%= config.ru.botdomain.replace(/\.$/, '') %>"
                                                                                           class="form-control input_url text-right" type="text"
                                                                                           placeholder="<%= config.language %>" style="border-right: 0">
                                                                                    <span class="input-group-addon" style="background-color: #1a2035; border-right: 0;border-left: 0;">
                                                                                        .
                                                                                    </span>
                                                                                    <input name="config.ru.bomain"
                                                                                           value="<%= config.ru.bomain %>"
                                                                                           class="form-control input_url text-left" type="text"
                                                                                           placeholder="<%= config.bomain ? config.bomain : config.domain %>" style="border-left: 0">
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </article>
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-sm-12">
                                            <div class="faq">
                                                <article class="faq-group">
                                                    <header class="text-muted text-left">
                                                        <i class="fa fa-plus"></i>
                                                        <h3><%= __('Поддомены для ботов') %></h3>
                                                    </header>
                                                    <div class="faq-content clearfix">
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <div class="cinema-box">
                                                                    <div class="cinema-box-header">
                                                                        <div class="cinema-label"><i class="fas fa-robot"></i>&nbsp;&nbsp;&nbsp;<%= __('Поддомены для ботов') %>
                                                                        </div>
                                                                        <div class="cinema-label hidden-xs" style="float: right">
                                                                            <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html"
                                                                               class="csstip"
                                                                               data-title="<%= __('Укажите для каждого фильма или для группы фильмов поддомен, который увидят только боты') %>"
                                                                               target="_blank">
                                                                                <i class="fa fa-info-circle"></i>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                    <div class="inner-padding">
                                                                        <div class="row">
                                                                            <div class="col-sm-12">
                                                                                <div class="advanced-textarea">
                                                                <textarea class="form-control" name="config.botdomains"
                                                                          placeholder="if (id === 301) { 'matrix' } else if (id > 500 && id < 1000) { 'cinema' } else { url }"><%= config.botdomains %></textarea>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </article>
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-sm-12">
                                            <div class="faq">
                                                <article class="faq-group">
                                                    <header class="text-muted text-left">
                                                        <i class="fa fa-plus"></i>
                                                        <h3><%= __('Автоматическое изменение поддомена') %></h3>
                                                    </header>
                                                    <div class="faq-content clearfix">
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <div class="cinema-box">
                                                                    <div class="cinema-box-header">
                                                                        <div class="cinema-label">
                                                                            <i class="fas fa-sort-numeric-down"></i>&nbsp;&nbsp;&nbsp;<%= __('Автоматическое изменение поддомена') %>
                                                                        </div>
                                                                        <div class="cinema-label hidden-xs" style="float: right">
                                                                            <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html"
                                                                               class="csstip"
                                                                               data-title="<%= __('Если домен попадает в реестр запрещенных сайтов, будет сделано автоматическое изменение поддомена, путем добавления цифр в конец поддомена (hd1, hd2, hd3 и т.п.)') %>"
                                                                               target="_blank">
                                                                                <i class="fa fa-info-circle"></i>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                    <div class="inner-padding">
                                                                        <div class="row">
                                                                            <div class="col-lg-3 col-xs-3 text-center">
                                                                                <div class="inline-labels">
                                                                                    <label>
                                                                                        <input name="config.blacklist" value="1"
                                                                                        <% if (config.blacklist === 1) { %> checked=""<% } %>
                                                                                               type="radio"><span></span>
                                                                                        <span><%= __('Для ботов и людей') %></span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-lg-3 col-xs-3 text-center">
                                                                                <div class="inline-labels">
                                                                                    <label>
                                                                                        <input name="config.blacklist" value="2"
                                                                                        <% if (config.blacklist === 2) { %> checked=""<% } %>
                                                                                               type="radio"><span></span>
                                                                                        <span><%= __('Для ботов') %></span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-lg-3 col-xs-3 text-center">
                                                                                <div class="inline-labels">
                                                                                    <label>
                                                                                        <input name="config.blacklist" value="3"
                                                                                        <% if (config.blacklist === 3) { %> checked=""<% } %>
                                                                                               type="radio"><span></span>
                                                                                        <span><%= __('Для людей') %></span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-lg-3 col-xs-3 text-center">
                                                                                <div class="inline-labels">
                                                                                    <label>
                                                                                        <input name="config.blacklist" value="0"
                                                                                        <% if (config.blacklist === 0) { %> checked=""<% } %>
                                                                                               type="radio"><span></span>
                                                                                        <span><%= __('Отключить') %></span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </article>
                                            </div>
                                        </div>

                                        <div class="spacer-10"></div>

                                        <div class="col-sm-12">
                                            <div class="faq">
                                                <article class="faq-group">
                                                    <header class="text-muted text-left">
                                                        <i class="fa fa-plus"></i>
                                                        <h3><%= __('Автоматическое прописывание поддомена в DNS') %></h3>
                                                    </header>
                                                    <div class="faq-content clearfix">
                                                        <div class="row">
                                                            <div class="col-sm-12">
                                                                <div class="cinema-box">
                                                                    <div class="cinema-box-header">
                                                                        <div class="cinema-label">
                                                                            <i class="far fa-keyboard"></i>&nbsp;&nbsp;&nbsp;<%= __('API CloudFlare') %>
                                                                        </div>
                                                                        <div class="cinema-label hidden-xs" style="float: right">
                                                                            <a href="https://CinemaPress.io/<%- config.language %>/admin/main.html"
                                                                               class="csstip"
                                                                               data-title="<%= __('Все поддомены, которые Вы указываете в админ-панели, будут автоматически прописаны в DNS CloudFlare') %>"
                                                                               target="_blank">
                                                                                <i class="fa fa-info-circle"></i>
                                                                            </a>
                                                                        </div>
                                                                    </div>
                                                                    <div class="inner-padding">
                                                                        <div class="row">
                                                                            <div class="col-xs-12">
                                                                                <div class="input-group">
                                                                                    <span class="input-group-addon">
                                                                                        <%= __('Email') %>:
                                                                                    </span>
                                                                                    <input name="config.dns.cloudflare.email" value="<%= config.dns.cloudflare.email %>" class="form-control" type="text" placeholder="user@protonmail.com" style="border-right: 0">
                                                                                </div>
                                                                            </div>

                                                                            <div class="spacer-10"></div>

                                                                            <div class="col-xs-12">
                                                                                <div class="input-group">
                                                                                    <span class="input-group-addon">
                                                                                        <%= __('Global API Key') %>:
                                                                                    </span>
                                                                                    <input name="config.dns.cloudflare.key" value="<%= config.dns.cloudflare.key %>" class="form-control" type="text" placeholder="22d259e410c39367633fba21988a7d45d05c8" style="border-right: 0">
                                                                                </div>
                                                                            </div>
                                                                        </div>

                                                                        <div class="spacer-10"></div>

                                                                        <div class="row">
                                                                            <div class="col-lg-6 col-xs-6 text-center">
                                                                                <div class="inline-labels">
                                                                                    <label>
                                                                                        <input name="config.dns.cloudflare.proxied" value="true" <% if (config.dns.cloudflare.proxied === 'true') { %> checked=""<% } %> type="radio"><span></span>
                                                                                        <span><%= __('Проксировать') %></span>
                                                                                    </label>
                                                                                </div>
                                                                            </div>
                                                                            <div class="col-lg-6 col-xs-6 text-center">
