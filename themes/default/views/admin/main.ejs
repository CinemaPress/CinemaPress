            <script>
              $('#one_slider').noUiSlider({
                range: [120, 2400],
                start: [<%= config.loadavg.one %>],
                handles: 1,
                step: 120,
                connect: 'lower',
                serialization: { resolution: 1, to: [$('#one')] }
              }), $('#five_slider').noUiSlider({
                range: [120, 2400],
                start: [<%= config.loadavg.five %>],
                handles: 1,
                step: 120,
                connect: 'lower',
                serialization: { resolution: 1, to: [$('#five')] }
              }), $('#fifteen_slider').noUiSlider({
                range: [120, 2400],
                start: [<%= config.loadavg.fifteen %>],
                handles: 1,
                step: 120,
                connect: 'lower',
                serialization: { resolution: 1, to: [$('#fifteen')] }
              });
              setInterval(function() {
                document.querySelector('#one_loadavg').innerHTML = '1 min: ' + document.querySelector('#one').value + '%';
              }, 100);
              setInterval(function() {
                document.querySelector('#five_loadavg').innerHTML = '5 min: ' + document.querySelector('#five').value + '%';
              }, 100);
              setInterval(function() {
                document.querySelector('#fifteen_loadavg').innerHTML = '15 min: ' + document.querySelector('#fifteen').value + '%';
              }, 100);

              function dayToLetter(random, now) {
                now = now || new Date();
                var year = now.getFullYear();
                var start = new Date(year, 0, 0);
                var diff = now - start;
                var oneDay = 86400000;
                var day = ('00' + Math.floor(diff / oneDay)).slice(-3);
                var letter1 = ['a', 'e', 'i', 'o', 'u'];
                var letter2 = ['b', 'c', 'd', 'f', 'g', 'h', 'z', 'k', 'l', 'm'];
                var letter3 = ['n', 'p', 'q', 'r', 's', 't', 'v', 'w', 'x', 'y']; // j
                if ('<%- config.domain.split('.')[0] %>'.length % 2) {
                  letter1 = letter1.reverse();
                }
                if ('<%- config.domain.split('.')[1] %>'.length % 2) {
                  letter2 = letter2.reverse();
                }
                var result =
                  letter2[parseInt(day[0])] +
                  letter1[parseInt(day[1]) % 5] +
                  letter3[parseInt(day[2])] +
                  (parseInt(random) === 3 ? '' : letter1[year % 5]);
                if (parseInt(day[1]) >= 5) {
                  result =
                    letter1[parseInt(day[0]) % 5] +
                    ((parseInt(day[0] + day[2]) % 2) ? letter2 : letter3)[parseInt(day[1])] +
                    letter1[parseInt(day[2]) % 5] +
                    (parseInt(random) === 3 ? '' : letter2[year % 10]);
                }
                if (now.getDate() % 2) {
                  result = result.split('').reverse().join('');
                }
                return result;
              }

              var radios = document.querySelectorAll('[name=\'config.random\']');
              if (radios) {
                radios.forEach(function(radio) {
                  radio.addEventListener('change', function() {
                    if (this.value !== '0') {
                      if (this.checked) {
                        document.querySelector('[name=\'config.subdomain\']').value = dayToLetter(this.value);
                        document.querySelector('#subdomains').style.display = 'block';
                        generate_domains(this.value);
                        document.querySelector('#current_subdomain').innerHTML = formatDate(new Date()) + ' - <code>' + dayToLetter(this.value, new Date()) + '.<%= config.domain %></code>';
                      }
                    } else {
                      if (this.checked) {
                        document.querySelector('#subdomains').style.display = 'none';
                      }
                    }
                  });
                  if (radio.value !== '0') {
                    if (radio.checked) {
                      document.querySelector('#subdomains').style.display = 'block';
                      generate_domains(radio.value);
                    }
                  } else {
                    if (radio.checked) {
                      document.querySelector('#subdomains').style.display = 'none';
                    }
                  }
                });
              }

              var radios2 = document.querySelectorAll('[name=\'config.ru.random\']');
              if (radios2) {
                radios2.forEach(function(radio) {
                  radio.addEventListener('change', function() {
                    document.querySelector('[name=\'config.ru.subdomain\']').value = dayToLetter(this.value).split('').reverse().join('');
                    if (document.querySelector('[name=\'config.ru.domain\']').value === '') {
                      document.querySelector('[name=\'config.ru.domain\']').value = '<%= config.domain %>';
                    }
                  });
                });
              }

              function generate_domains(random) {
                var list_subdomains1 = document.querySelector('.list_subdomains1');
                var list_subdomains2 = document.querySelector('.list_subdomains2');
                var list_subdomains3 = document.querySelector('.list_subdomains3');
                list_subdomains1.innerHTML = '';
                list_subdomains2.innerHTML = '';
                list_subdomains3.innerHTML = '';
                for (var i = 1; i <= 183; i++) {
                  var tomorrow = new Date();
                  tomorrow.setDate(tomorrow.getDate() + i);
                  var div = document.createElement('div');
                  var day = document.createElement('span');
                  var sub = document.createElement('code');
                  day.innerHTML = formatDate(tomorrow);
                  sub.innerHTML = dayToLetter(random, tomorrow);
                  div.appendChild(day);
                  div.appendChild(sub);
                  if (i <= 61) {
                    list_subdomains1.appendChild(div);
                  } else if (i <= 122) {
                    list_subdomains2.appendChild(div);
                  } else if (i <= 183) {
                    list_subdomains3.appendChild(div);
                  }
                }
              }

              function formatDate(date) {
                var monthNames = [
                  'January', 'February', 'March',
                  'April', 'May', 'June', 'July',
                  'August', 'September', 'October',
                  'November', 'December'
                ];
                var day = date.getDate();
                var monthIndex = date.getMonth();
                var year = date.getFullYear();
                return day + ' ' + monthNames[monthIndex] + ' ' + year;
              }
            </script>

