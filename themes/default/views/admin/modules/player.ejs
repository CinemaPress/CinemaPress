                                                                            <% if (typeof d[2] !== 'undefined') { %>
                                                                                ~ <span class="translate"><%= d[2] || '' %></span>
                                                                            <% } %>
                                                                            <% if (typeof d[3] !== 'undefined') { %>
                                                                                ~ <span class="quality"><%= d[3] || '' %></span>
                                                                            <% } %>
                                                                            <% if (typeof d[4] !== 'undefined') { %>
                                                                                ~ <span class="season"><%= d[4] || '' %></span>
                                                                            <% } %>
                                                                            <% if (typeof d[5] !== 'undefined') { %>
