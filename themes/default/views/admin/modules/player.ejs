                                                            <script>
                                                              document.addEventListener('DOMContentLoaded', function() {
                                                                var json = document.querySelector('[name="modules.player.data.embed.dataset"]');
                                                                if (json) {
                                                                  jsonPrettyEmbed();
                                                                  json.oninput = function() {
                                                                    jsonPrettyEmbed();
                                                                  };
                                                                }

                                                                var customAdd = document.querySelector('#embedAdd');
                                                                customAdd.addEventListener('click', function() {
                                                                  var customKey = document.querySelector('#embedKey');
                                                                  var customValue = document.querySelector('#embedValue');
                                                                  if (customKey && customKey.value && customValue && customValue.value) {
                                                                    var customString = document.querySelector('[name="modules.player.data.embed.dataset"]').value;
                                                                    var customJson = JSON.parse(customString || '{}');
                                                                    document.querySelector('[name="modules.player.data.embed.dataset"]').value =
                                                                      JSON.stringify(expandEmbed(
                                                                        customJson,
                                                                        customKey.value,
                                                                        (customValue.value === 'true')
                                                                          ? true
                                                                          : (customValue.value === 'false')
                                                                          ? false
                                                                          : (!customValue.value.replace(/[0-9]/g, ''))
                                                                            ? parseInt(customValue.value)
                                                                            : /^\[.*]$/.test(customValue.value) && customValue.value.replace(/[a-z0-9\]\[_]/g, '')
                                                                              ? JSON.parse(customValue.value.replace(/'/g, '"'))
                                                                              : customValue.value
                                                                        )
                                                                      );
                                                                    jsonPrettyEmbed();
                                                                    customKey.value = '';
                                                                    customValue.value = '';
                                                                  }
                                                                });
                                                              });

                                                              function jsonPrettyEmbed() {
                                                                var ugly = document.querySelector('[name="modules.player.data.embed.dataset"]').value;
                                                                if (!ugly) return;
                                                                ugly = JSON.parse(ugly);
                                                                var cnt = 0;
                                                                c(ugly);
                                                                document.querySelector('[name="modules.player.data.embed.dataset"]').style.height = (50 + cnt * 20) + 'px';
                                                                document.querySelector('[name="modules.player.data.embed.dataset"]').value =
                                                                  JSON.stringify(ugly, undefined, 4);

                                                                function c(obj) {
                                                                  Object.keys(obj).forEach(function(val) {
                                                                    if (typeof obj[val] === 'object') {
                                                                      c(obj[val]);
                                                                      cnt++;
                                                                    }
                                                                    cnt++;
                                                                  });
                                                                }
                                                              }

                                                              function expandEmbed(model, name, value) {
                                                                var items = name.split('.');
                                                                var output = model;
                                                                var ref = output;
                                                                for (var i = 0; i < items.length - 1; i++) {
                                                                  ref[items[i]] = (ref[items[i]])
                                                                    ? ref[items[i]]
                                                                    : {};
                                                                  ref = ref[items[i]];
                                                                }
                                                                ref[items[items.length - 1]] = value;
                                                                return output;
                                                              }
                                                            </script>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </article>
                                </div>
                            </div>
                        </div>

                        <script>
                          document.addEventListener('DOMContentLoaded', function() {
                            [
                              'information',
                              'trailer',
                              'online',
                              'trailer',
                              'download',
                              'picture'
                            ].forEach(function(tab) {
                              var json = document.querySelector('[name="modules.player.data.cinemaplayer.' + tab + '.dataset"]');
                              if (json) {
                                jsonPrettyTab(tab);
                                json.oninput = function() {
                                  jsonPrettyTab(tab);
                                };
                              }

                              var customAdd = document.querySelector('#' + tab + 'Add');
                              if (!customAdd) return;
                              customAdd.addEventListener('click', function() {
                                var customKey = document.querySelector('#' + tab + 'Key');
                                var customValue = document.querySelector('#' + tab + 'Value');
                                if (customKey && customKey.value && customValue && customValue.value) {
                                  var customString = document.querySelector('[name="modules.player.data.cinemaplayer.' + tab + '.dataset"]').value;
                                  var customJson = JSON.parse(customString || '{}');
                                  document.querySelector('[name="modules.player.data.cinemaplayer.' + tab + '.dataset"]').value =
                                    JSON.stringify(expandTab(
                                      customJson,
                                      customKey.value,
                                      (customValue.value === 'true')
                                        ? true
                                        : (customValue.value === 'false')
                                        ? false
                                        : (!customValue.value.replace(/[0-9]/g, ''))
                                          ? parseInt(customValue.value)
                                          : /^\[.*]$/.test(customValue.value) && customValue.value.replace(/[a-z0-9\]\[_]/g, '')
                                            ? JSON.parse(customValue.value.replace(/'/g, '"'))
                                            : customValue.value
                                      )
                                    );
                                  jsonPrettyTab(tab);
                                  customKey.value = '';
                                  customValue.value = '';
                                }
                              });
                            });
                          });

                          function jsonPrettyTab(tab) {
                            var ugly = document.querySelector('[name="modules.player.data.cinemaplayer.' + tab + '.dataset"]').value;
                            if (!ugly) return;
                            ugly = JSON.parse(ugly);
                            var cnt = 0;
                            c(ugly);
                            document.querySelector('[name="modules.player.data.cinemaplayer.' + tab + '.dataset"]').style.height = (50 + cnt * 20) + 'px';
                            document.querySelector('[name="modules.player.data.cinemaplayer.' + tab + '.dataset"]').value =
                              JSON.stringify(ugly, undefined, 4);

                            function c(obj) {
                              Object.keys(obj).forEach(function(val) {
                                if (typeof obj[val] === 'object') {
                                  c(obj[val]);
                                  cnt++;
                                }
                                cnt++;
                              });
                            }
                          }

                          function expandTab(model, name, value) {
                            var items = name.split('.');
                            var output = model;
                            var ref = output;
                            for (var i = 0; i < items.length - 1; i++) {
                              ref[items[i]] = (ref[items[i]])
                                ? ref[items[i]]
                                : {};
                              ref = ref[items[i]];
                            }
                            ref[items[items.length - 1]] = value;
                            return output;
                          }
                        </script>

                    <% } else { %>

                        <div class="row">
                            <div class="col-xs-12">
                                <div class="attention-box attention-danger">
                                    <h4><%= __('Модуль') %> «<%- title %>» <%= __('отключен') %></h4>
                                    <div class="spacer-20"></div>
                                    <p class="text-muted">
                                        <%= __('Модуль позволяет добавить на веб-сайт плеер для онлайн просмотра фильмов.') %>
                                        <br>
                                        <%= __('Чтобы включить модуль, нажмите на кнопку «Активировать» вверху экрана.') %>
                                    </p>
                                </div>
                            </div>
                        </div>

                    <% } %>

                </div>
            </div>

            <%- include('../includes/footer.ejs'); -%>

        </div>
    </div>
</div>

</body>
</html>